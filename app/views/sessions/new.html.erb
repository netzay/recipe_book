
<h2>Login:</h2>


<%= form_tag '/login' do %>

<div>Name:</div> <div><%= text_field_tag :name %></div>
<div>Password:</div> <div><%= password_field_tag :password %></div>
<%= submit_tag "Submit" %>
<% end %>
<%=link_to("Sign-Up", new_user_path)%><br>

<%= link_to "Sign in with Google", "/auth/google_oauth2", id: "sign_in" %>